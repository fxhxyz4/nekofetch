{
	"name": "nekofetch",
	"version": "1.0.0",
	"description": "nekofetch",
	"keywords": [
		"neofetch",
		"nekofetch",
		"nodejs pkg",
		"system info"
	],
	"type": "commonjs",
	"main": "./main.js",
	"bin": "./main.js",
	"author": {
		"name": "fxhxyz4",
		"email": "fxhsec@proton.me",
		"url": "https://fxhxyz.vercel.app/"
	},
	"homepage": "https://github.com/fxhxyz4/nekofetch/wiki/",
	"bugs": "https://github.com/fxhxyz4/nekofetch/issues",
	"repository": {
		"type": "git",
		"url": "git://github.com/fxhxyz4/nekofetch.git"
	},
	"maintainers": [
		{
			"name": "fxhxyz4",
			"email": "fxhsec@proton.me"
		}
	],
	"os": [
		"linux",
		"win32",
		"darwin"
	],
	"license": "MIT",
	"scripts": {
		"dev": "nodemon .",
		"dev-config": "cross-env CONFIG_PATH=$HOME/Desktop/dev/nekofetch/config/default.json nodemon .",
		"build-linux": "pkg . --experimental-modules --targets node18-linux-x64 --output ./build/nekofetch",
    "build-win": "pkg . --experimental-modules --targets node18-win-x64 --output ./build/nekofetch",
    "build-mac": "pkg . --experimental-modules --targets node18-macos-x64 --output ./build/nekofetch",
    "build": "npm run build-linux && npm run build-win && npm run build-mac",
		"test": "echo test"
	},
	"engines": {
		"node": "18.x"
	},
	"devDependencies": {
		"nodemon": "^3.1.9"
	},
	"dependencies": {
		"asciify-image": "^0.1.10",
		"cross-env": "^7.0.3",
		"pkg": "^5.8.1"
	},
	"pkg": {
		"assets": [
			"assets/*",
			"config/*"
		]
	}
}
